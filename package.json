{
  "name": "react-isomorphic-starterkit",
  "description": "Isomorphic starterkit with server-side React rendering.",
  "version": "5.4.0",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/RickWong/react-isomorphic-starterkit.git"
  },
  "homepage": "https://github.com/RickWong/react-isomorphic-starterkit",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "starter",
    "boilerplate",
    "template",
    "webpack",
    "koa",
    "transmit"
  ],
  "main": "dist/server.js",
  "scripts": {
    "start": "forever --minUptime 1000 --spinSleepTime 1000 -c \"node --harmony\" ./dist/server.js",
    "build-server": "webpack --colors --display-error-details --config configs/webpack.server.js",
    "build-client": "webpack --colors --display-error-details --config configs/webpack.client.js",
    "build": "concurrently \"npm run build-server\" \"npm run build-client\"",
    "watch-server": "webpack --watch --verbose --colors --display-error-details --config configs/webpack.server-watch.js",
    "watch-server-start": "node node_modules/just-wait -t 50000 --pattern \"dist/*.js\" && npm run start",
    "watch-client": "webpack-dev-server --config configs/webpack.client-watch.js",
    "watch": "concurrently --kill-others \"npm run watch-server-start\" \"npm run watch-server\" \"npm run watch-client\""
  },
  "dependencies": {
    "animakit-rotator": "^2.0.0",
    "archiver": "^1.3.0",
    "async": "^2.1.4",
    "babel-core": "^6.10.4",
    "babel-polyfill": "6.22.0",
    "bcrypt": "^1.0.1",
    "body-parser": "^1.15.2",
    "concurrently": "^3.1.0",
    "crypto": "0.0.3",
    "css-loader": "0.26.1",
    "eventemitter2": "^3.0.0",
    "express": "^4.14.0",
    "fdf": "0.0.0",
    "file-loader": "0.9.0",
    "flux-react": "^3.0.0",
    "formidable": "^1.0.17",
    "fs-extra": "^2.0.0",
    "intl": "^1.2.5",
    "isomorphic-fetch": "2.2.1",
    "isomorphic-style-loader": "1.1.0",
    "jquery": "^3.1.1",
    "jszip": "^3.1.3",
    "koa": "1.2.4",
    "koa-proxy": "0.7.0",
    "koa-static": "2.0.0",
    "magic-globals": "^0.5.1",
    "material-ui": "^0.16.0",
    "mysql": "^2.11.1",
    "node-fetch": "^1.6.3",
    "node-sass": "4.3.0",
    "pdffiller": "^0.4.0",
    "react": "^15.1.0",
    "react-addons-css-transition-group": "^15.1.0",
    "react-addons-update": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-easy-transition": "0.0.5",
    "react-iframe": "0.0.4",
    "react-inline-css": "2.3.0",
    "react-layout-components": "^3.0.0",
    "react-loading-overlay": "^0.2.3",
    "react-mailto": "^0.4.0",
    "react-router": "3.0.2",
    "react-router-transition": "0.0.6",
    "react-tap-event-plugin": "^2.0.1",
    "react-transmit": "3.2.0",
    "react-webstorage": "0.0.4",
    "sass-loader": "4.1.1",
    "style-loader": "0.13.1",
    "styled-components": "^1.1.2"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.22.0",
    "concurrently": "3.1.0",
    "forever": "0.15.3",
    "json-loader": "0.5.4",
    "just-wait": "1.0.9",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-node-externals": "1.5.4"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
